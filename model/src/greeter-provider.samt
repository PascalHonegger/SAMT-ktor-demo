package tools.samt.greeter

provide GreeterEndpoint {
    implements Greeter

    transport http {
        operations: {
            Greeter: {
                greet: "POST /greet/{name} {header:binary} {cookie:type} {query:anotherString}",
                greetAll: "GET /greet/all {query:names}",
                greeting: "GET /greeting/{name}"
            }
        }
    }
}
